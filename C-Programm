#include <stdio.h>
#include <stdbool.h>

int main() {
    int a = 10;
    float b = 3.14f;
    double c = 9.81;
    char d = 'A';
    bool e = true;
    printf("int: %d\n", a);
    printf("float: %.2f\n", b);
    printf("double: %.2lf\n", c);
    printf("char: %c\n", d);
    printf("bool: %d\n", e);
    return 0;
}
